            <string>var item = global.inventory[slot];
var click = keyboard_check_pressed(ord('E'));
var equip1 = mouse_check_button_pressed(mb_left);
var equip2 = mouse_check_button_pressed(mb_right);

if obj_inventory.active = true {
    if (abs(mouse_x - x) &lt; 16) and (abs(mouse_y - y) &lt; 16) { //within 16 pixels of button
        draw_set_colour(c_white);
        draw_rectangle(x - 16, y - 16, x + 16 , y + 16, 0)
        if (click) {
            if (item != -1) {
                scr_item_drop_slot(slot);
            }
            if (item_mouse != -1) {
                scr_item_pickup_slot(item_mouse,slot)
            }
            item_mouse = item;
        }
        if (equip1) {
            if item_left = item {
                item_left = -1
            } else {
                item_left = item
            }
            if item_right = item {
                item_right = -1
            }
        }
        if (equip2) {
            if item_right = item {
                item_right = -1
            } else {
                item_right = item
            }
            if item_left = item {
                item_left = -1
            }
        }
    }
    if (item_left = item) and (item_left != -1) {
        draw_sprite(spr_inv_border_equip, 0, x, y)
    }
    if (item_right = item) and (item_right != -1) {
        draw_sprite(spr_inv_border_equip, 1, x, y)
    }
    if (item != -1) {
        draw_sprite(spr_items, item, x, y);
        if (item = 3) {
            draw_text(x -15, y - 15, string((obj_inventory.rifle_ammo) -1));
        }
        if item = 5 {
            draw_text(x -15, y - 15, string((obj_inventory.bp) ));
        }
        if item = 7 {
            draw_text(x -15, y - 15, string((obj_inventory.flare_ammo) ));
        }
        if item = 9 {
            draw_text(x -15, y - 15, string((obj_inventory.firebomb_ammo) ));
        }
    }
}
    


</string>
